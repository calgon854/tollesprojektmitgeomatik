{
  "builds": [
    {
      "src": "./api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "./pages/**/*.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}